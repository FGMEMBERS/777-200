<PropertyList>

<path>APpanel.ac</path>

<!--  Upper Panel Autopilot Controls-->

        <animation>
        <type>rotate</type>
        <object-name>BLimit.knob</object-name>
        <property>instrumentation/flightdirector/bank-limit-switch</property>
        <factor>-20.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.009</y-m>
            <z-m>-0.008</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>    

    <animation>
        <type>textranslate</type>
        <object-name>ias.flag</object-name>
        <property>instrumentation/flightdirector/mach-display</property>
        <factor>0.0589</factor>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
