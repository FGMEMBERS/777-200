            </not>
        </condition>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FDtoggle</object-name>
        <property>instrumentation/flightdirector/serviceable</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>THARM.switch</object-name>
        <property>autopilot/locks/speed-engaged</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.220</y-m>
            <z-m>0.031</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>HDGtoggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/flightdirector/lnav</property>
                    <value>1</value>
                </equals>
                <equals>
                    <property>instrumentation/flightdirector/lnav</property>
                    <value>2</value>
                </equals>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LNAV.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/lnav</property>
                <value>3</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ALTtoggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/vnav</property>
                <value>2</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ATtoggle</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/flightdirector/speed</property>
                <value>4</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VNAV.toggle</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>instrumentation/flightdirector/vnav</property>
                    <value>3</value>
                </greater-than>
                <less-than>
                    <property>instrumentation/flightdirector/vnav</property>
                    <value>7</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FLCH.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/vnav</property>
                <value>8</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LOC.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/lnav</property>
                <value>4</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APP.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/lnav</property>
                <value>10</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VS.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/flightdirector/vnav</property>
                <value>3</value>
            </equals>
        </condition>
    </animation>


