<animation>
   <type>select</type>
   <object-name>FDbars</object-name>
   <object-name>fd.annun</object-name>
   <condition>
       <property>instrumentation/flightdirector/serviceable</property>
    </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>GSscale</object-name>
   <object-name>GSneedle</object-name>
   <condition>
     <and>
         <property>/instrumentation/nav/has-gs</property>
         <property>/instrumentation/nav/in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>GSneedle</object-name>
   <property>/instrumentation/nav/gs-needle-deflection</property>
<interpolation>
     <entry>
       <ind> -10.0</ind>
       <dep>-0.0308</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 10.0</ind>
       <dep>0.0308</dep>
     </entry>
</interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>


 <animation>
   <type>select</type>
   <object-name>Omarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/outer</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>Mmarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/middle</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>Imarker</object-name>
   <condition>
     <equals>
       <property>/instrumentation/marker-beacon/inner</property>
       <value>true</value>
     </equals>
   </condition>
 </animation>
 
 <animation>
   <type>select</type>
   <object-name>LOCscale</object-name>
   <object-name>LOCneedle</object-name>
   <condition>
     <and>
         <property>/instrumentation/nav/nav-loc</property>
         <property>/instrumentation/nav/in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>LOCneedle</object-name>
   <property>/instrumentation/nav/heading-needle-deflection</property>
<interpolation>
     <entry>
       <ind> -10.0</ind>
       <dep>-0.0253</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 10.0</ind>
       <dep>0.0253</dep>
     </entry>
</interpolation> 
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>



 <animation>
   <type>rotate</type>
   <object-name>pfdhdg</object-name>
   <property>autopilot/internal/fdm-heading-bug-error-deg</property>
   <interpolation>
     <entry>
       <ind> -180.0</ind>
       <dep>-50.0</dep>
     </entry>
     <entry>
       <ind>-50.0</ind>
       <dep>-50.0</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 50.0</ind>
       <dep>50.0</dep>
     </entry>
     <entry>
       <ind>180.0</ind>
       <dep>50.0</dep>
     </entry>
   </interpolation>
<axis>
     <x>-1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.013</y-m>
     <z-m>-0.135</z-m>
   </center>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>FDbars</object-name>
   <property>autopilot/internal/target-pitch-deg</property>
<factor>0.00171</factor>
<axis>
     <x>0.0</x>
     <y>0.0</y>
     <z>1.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>FDbars</object-name>
   <property>autopilot/internal/target-roll-deg</property>
   <axis>
     <x>-1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>FDbars</object-name>
   <property>orientation/roll-deg</property>
   <axis>
     <x>1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>FDbars</object-name>
   <property>orientation/pitch-deg</property>
<factor>0.00171</factor>
<axis>
     <x>0.0</x>
     <y>0.0</y>
     <z>-1.0</z>
   </axis>
   <center>
     <x-m>0.007</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>

 <animation>
   <type>texmultiple</type>
   <object-name>AI</object-name>
   <transform>
     <property>/orientation/pitch-deg</property>
     <subtype>textranslate</subtype>
     <factor>0.00314</factor>
     <axis>
       <x>0</x>
       <y>1</y>
       <z>0</z>
     </axis>
   </transform>
   <transform>
     <property>/orientation/roll-deg</property>
     <subtype>texrotate</subtype>
     <center>
       <x>0.25</x>
       <y>0.50</y>
     </center>
     <axis>
       <x>0</x>
       <y>0</y>
       <z>-1</z>
     </axis>
   </transform>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>rollptr</object-name>
   <property>/orientation/roll-deg</property>
   <axis>
     <x>1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.0</x-m>
     <y-m>-0.014</y-m>
     <z-m>0.005</z-m>
   </center>
 </animation>



  <animation>
   <type>texrotate</type>
   <object-name>HDGface</object-name>
   <property>/orientation/heading-magnetic-deg</property>
   <center>
     <x>0.75</x>
     <y>0.75</y>
     <z>0.0</z>
   </center>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
 </animation>

