   <object-name>rdr.002</object-name>
   <property>controls/flight/rudder-trim</property>
   <factor>1.0</factor>
   <step>0.1</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

  <animation>
   <type>textranslate</type>
   <object-name>rdr.003</object-name>
   <property>controls/flight/rudder-trim</property>
   <factor>10.0</factor>
