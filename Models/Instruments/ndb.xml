<!--
777-200 Secondary Display 
-->

<PropertyList>
 <path>ndb.ac</path>

  <animation>
  <type>material</type>
  <object-name>NDB2</object-name>
  <emission>
    <red>0.3</red>
    <green>0.25</green>
    <blue>0.2</blue>
    <factor-prop>controls/lighting/map-lights</factor-prop>	
  </emission>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Rudder.ptr</object-name>
<property>surface-positions/rudder-pos-norm</property>
<factor>0.0242</factor>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Lail.ptr</object-name>
<property>surface-positions/left-aileron-pos-norm</property>
<factor>0.0118</factor>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Rail.ptr</object-name>
<property>surface-positions/right-aileron-pos-norm</property>
<factor>0.0118</factor>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Lflap.ptr</object-name>
<property>surface-positions/flap-pos-norm</property>
<factor>0.0235</factor>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Rflap.ptr</object-name>
<property>surface-positions/flap-pos-norm</property>
<factor>0.0235</factor>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
</animation>

  <animation>
  <type>textranslate</type>
  <object-name>spoiler.1</object-name>
<property>surface-positions/speedbrake-norm</property>
    <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>0.2</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>0.4</ind>
      <dep>0.0339</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.0339</dep>
    </entry>
</interpolation>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
</animation>

  <animation>
  <type>textranslate</type>
  <object-name>spoiler.2</object-name>
<property>surface-positions/speedbrake-norm</property>
    <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>0.6</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>0.8</ind>
      <dep>0.0339</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.0339</dep>
    </entry>
</interpolation>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
</animation>

  <animation>
  <type>translate</type>
  <object-name>Elev.ptr</object-name>
<property>surface-positions/elevator-pos-norm</property>
<factor>0.0118</factor>
   <axis>
     <x>0</x>
     <y>0</y>
     <z>1</z>
   </axis>
</animation>


  <animation>
   <type>select</type>
   <object-name>stabtxt</object-name>
<condition>
<less-than>   
<property>controls/flight/elevator-trim</property>
   <value>0</value>
</less-than>
</condition>
 </animation>

  <animation>
   <type>textranslate</type>
   <object-name>stabtxt.001</object-name>
   <property>controls/flight/elevator-trim</property>
   <factor>0.1</factor>
   <step>1</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

  <animation>
   <type>textranslate</type>
   <object-name>stabtxt.002</object-name>
   <property>controls/flight/elevator-trim</property>
   <factor>1.0</factor>
   <step>0.1</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

  <animation>
   <type>textranslate</type>
   <object-name>stabtxt.003</object-name>
   <property>controls/flight/elevator-trim</property>
