<?xml version="1.0"?>


<PropertyList>
    <name>PFDtext</name>
    <w-base>256</w-base>
    <h-base>256</h-base>

    <layers>

        <layer>
            <name>SPD annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.3</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>autopilot/locks/speed</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-77</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>110</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>ROLL annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.3</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>autopilot/locks/heading</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-30</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>110</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>PITCH annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.3</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>autopilot/locks/altitude</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>12</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>110</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>FD on</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.3</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/flightdirector/AP-annun</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-30</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>72</offset>
                </transformation>
            </transformations>
        </layer>


        <layer>
            <name>mach</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>velocities/mach</property>
                    <format>%0.3f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-118</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-100</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>airspeed</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>velocities/airspeed-kt</property>
                    <format>%03.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-122</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-0.4</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>altitude</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>8</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>instrumentation/altimeter/indicated-altitude-ft</property>
                    <format>%05.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>69</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-0.4</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>inhg</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>6</point-size>
            <color>
                <red>0.3</red>
                <green>0.8</green>
                <blue>0.6</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>instrumentation/altimeter/setting-inhg</property>
                    <format>%2.2f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>65</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-101</offset>
                </transformation>
            </transformations>
        </layer>


</layers>

</PropertyList>