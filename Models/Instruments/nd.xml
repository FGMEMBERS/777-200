 <animation>
   <type>textranslate</type>
   <object-name>hdgtext</object-name>
   <property>/orientation/heading-magnetic-deg</property>
   <factor>0.001</factor>
   <step>100</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>hdgtext.001</object-name>
   <property>/orientation/heading-magnetic-deg</property>
   <factor>0.01</factor>
   <step>10</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>hdgtext.002</object-name>
   <property>/orientation/heading-magnetic-deg</property>
   <factor>0.1</factor>
   <step>1</step>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

