        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>Panel</object-name>
        <condition>
            <less-than>
                <property>rendering/scene/ambient/blue</property>
                <value>0.25</value>
            </less-than>
        </condition>
        <property>controls/lighting/master-panel</property>
        <factor>-0.0840</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
