        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.033</ind>
                <dep>11.8</dep>
            </entry>
            <entry>
                <ind>0.166</ind>
                <dep>23.5</dep>
            </entry>
            <entry>
                <ind>0.5</ind>
                <dep>35.50</dep>
            </entry>
            <entry>
                <ind>0.666</ind>
                <dep>48.0</dep>
            </entry>
            <entry>
                <ind>0.833</ind>
                <dep>60.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>72.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-23.185</x-m>
            <y-m>0</y-m>
            <z-m>0.011</z-m>
        </center>
        <axis>
            <x>0</x>
