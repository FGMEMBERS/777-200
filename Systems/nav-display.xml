<?xml version="1.0"?>

<!--
**********************************************************************
Configure the nav-display symbology
**********************************************************************
-->

<PropertyList>

<symbol>
    <type type="string">airport</type>
    <enable>
        <property>instrumentation/efis/inputs/arpt</property>
    </enable>
    
    <state type="string">!departure</state>
    <state type="string">!arrival</state>
    <width type="float">16</width>
    <texture-size type="int">512</texture-size>
    <u0 type="float">8</u0>
    <v0 type="float">8</v0>
    <u1 type="float">56</u1>
    <v1 type="float">56</v1>
    <color>
        <red type="float">0</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>18</text-offset-x>
    <text-offset-y>0</text-offset-y>
</symbol>

<symbol>
    <type type="string">runway</type>
    <state type="string">departure</state>
    <enable>
        <less-than>
            <property>instrumentation/efis/inputs/range</property>
            <value>41.0</value>
        </less-than>
    </enable>
    
    <x0 type="float">-12</x0>
    <y0 type="float">-20</y0>
    <x1 type="float">12</x1>
    <y1 type="float">4</y1>
    <y2 type="float">-4</y2>
    <y3 type="float">20</y3>
    
    <rotate-to-heading type="bool">true</rotate-to-heading>
    <stretch-symbol type="bool">true</stretch-symbol>
    
    <texture-size type="int">512</texture-size>
    <u0 type="float">12</u0>
    <v0 type="float">322</v0>
    <u1 type="float">54</u1>
    <v1 type="float">378</v1>
    <v2 type="float">420</v2>
    <v3 type="float">490</v3>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
</symbol>

<symbol>
    <type type="string">runway</type>
    <state type="string">arrival</state>
    <enable>
        <less-than>
            <property>instrumentation/efis/inputs/range</property>
            <value>41.0</value>
        </less-than>
    </enable>
    
    <x0 type="float">-12</x0>
    <y0 type="float">-20</y0>
    <x1 type="float">12</x1>
    <y1 type="float">4</y1>
    <y2 type="float">-4</y2>
    <y3 type="float">20</y3>
    
    <rotate-to-heading type="bool">true</rotate-to-heading>
    <stretch-symbol type="bool">true</stretch-symbol>
    
    <texture-size type="int">512</texture-size>
    <u0 type="float">12</u0>
    <v0 type="float">322</v0>
    <u1 type="float">54</u1>
    <v1 type="float">378</v1>
    <v2 type="float">420</v2>
    <v3 type="float">490</v3>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
</symbol>

<symbol>
    <type type="string">runway</type>
    <state type="string">departure</state>
    <enable>
        <greater-than>
            <property>instrumentation/efis/inputs/range</property>
            <value>40.0</value>
        </greater-than>
    </enable>
    
    <x0 type="float">-12</x0>
    <y0 type="float">-12</y0>
    <x1 type="float">12</x1>
    <y1 type="float">24</y1>
    
    <rotate-to-heading type="bool">true</rotate-to-heading>
    <texture-size type="int">512</texture-size>
    <u0 type="float">195</u0>
    <v0 type="float">260</v0>
    <u1 type="float">241</u1>
    <v1 type="float">328</v1>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
</symbol>

<symbol>
    <type type="string">runway</type>
    <state type="string">arrival</state>
    <enable>
        <greater-than>
            <property>instrumentation/efis/inputs/range</property>
            <value>40.0</value>
        </greater-than>
    </enable>
    
    <x0 type="float">-12</x0>
    <y0 type="float">-12</y0>
    <x1 type="float">12</x1>
    <y1 type="float">24</y1>
    
    <rotate-to-heading type="bool">true</rotate-to-heading>
    <texture-size type="int">512</texture-size>
    <u0 type="float">195</u0>
    <v0 type="float">260</v0>
    <u1 type="float">241</u1>
    <v1 type="float">328</v1>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
</symbol>

<symbol>
    <type type="string">runway</type>
    <state type="string">arrival</state>
    <width type="float">16</width>
    <rotate-to-heading type="bool">true</rotate-to-heading>
    <texture-size type="int">512</texture-size>
    <u0 type="float">64</u0>
    <v0 type="float">8</v0>
    <u1 type="float">128</u1>
    <v1 type="float">56</v1>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
</symbol>

<symbol>
    <type type="string">waypoint</type>
    <state type="string">current-wp</state>
    
    <width type="float">24</width>
    <texture-size  type="int">512</texture-size>
    <u0 type="float">8</u0>
    <v0 type="float">64</v0>
    <u1 type="float">56</u1>
    <v1 type="float">128</v1>
    <color>
        <red type="float">1</red>
        <green type="float">0</green>
        <blue type="float">1</blue>
    </color>
    <line-color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </line-color>
    <draw-leg type="bool">true</draw-leg>
    
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>18</text-offset-x>
    <text-offset-y>18</text-offset-y>
</symbol>
    
<symbol>
    <type type="string">waypoint</type>
    <state type="string">on-active-route</state>
    <state type="string">!current-wp</state>
    <width type="float">24</width>
    <texture-size  type="int">512</texture-size>
    <u0 type="float">8</u0>
    <v0 type="float">64</v0>
    <u1 type="float">56</u1>
    <v1 type="float">128</v1>
    <color>
        <red type="float">1</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
    <draw-leg type="bool">true</draw-leg>
    
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>18</text-offset-x>
    <text-offset-y>18</text-offset-y>
</symbol>
    
<symbol>
    <type type="string">VOR</type>
    <state type="string">tuned</state>
    <width type="float">16</width>
    <texture-size  type="int">512</texture-size>
    <u0 type="float">64</u0>
    <v0 type="float">64</v0>
    <u1 type="float">128</u1>
    <v1 type="float">128</v1>
    <color>
        <red type="float">0</red>
        <green type="float">1</green>
        <blue type="float">0</blue>
    </color>
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>18</text-offset-x>
    <text-offset-y>0</text-offset-y>
</symbol>

<symbol>
    <type type="string">VOR</type>
    <enable>
        <property>instrumentation/efis/inputs/sta</property>
    </enable>
    <state type="string">!tuned</state>
    <width type="float">16</width>
    <texture-size  type="int">512</texture-size>
    <u0 type="float">64</u0>
    <v0 type="float">64</v0>
    <u1 type="float">128</u1>
    <v1 type="float">128</v1>
    <color>
        <red type="float">0</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>18</text-offset-x>
    <text-offset-y>0</text-offset-y>
</symbol>

<!-- inactive waypoints, shown when WPT button is selected -->
<symbol>
    <type type="string">fix</type>
    <enable>
        <property>instrumentation/efis/inputs/wpt</property>
    </enable>
    <state type="string">!on-active-route</state>
    <width type="float">16</width>
    <texture-size  type="int">512</texture-size>
    <u0 type="float">0</u0>
    <v0 type="float">256</v0>
    <u1 type="float">64</u1>
    <v1 type="float">320</v1>
    <color>
        <red type="float">0</red>
        <green type="float">1</green>
        <blue type="float">1</blue>
    </color>
    <text type="string">{id}</text>
    <text-align type="string">left-top</text-align>
    <text-offset-x>16</text-offset-x>
    <text-offset-y>16</text-offset-y>
</symbol>

